<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

	<parameter name="randomSeed"
		displayName="Default Random Seed" type="int" defaultValue="__NULL__" />

	<parameter name="susceptible_count" displayName="Susceptible Count"
		type="int" defaultValue="1200" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />


	<parameter name="infectious_count" displayName="Infectious Count"
		type="int" defaultValue="15" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
		
	<parameter name="infection_rate" displayName="Infection Rate"
		type="float" defaultValue="0.5" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$FloatConverter" />
		
		
	<parameter name="recover_rate" displayName="Recover Rate"
		type="float" defaultValue="0.05" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$FloatConverter" />
</parameters>
